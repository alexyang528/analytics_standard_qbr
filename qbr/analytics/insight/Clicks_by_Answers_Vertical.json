{
    "$id": "qbr_Clicks_by_Answers_Vertical",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Clicks by Answers Vertical",
    "description": "Total Clicks by Answers Vertical",
    "query": {
        "metrics": [
            "yext/ANSWERS_CLICKS"
        ],
        "dimensions": [
            "yext/ANSWERS_VERTICAL_RETURNED"
        ],
        "filters": [
            {
                "filter": "yext/date",
                "in": "WEEKS_12"
            },
            {
                "filter": "yext/ANSWERS_TRAFFIC_TYPE",
                "in": [
                    "EXTERNAL"
                ]
            },
            {
                "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
                "in": [
                    "PRODUCTION"
                ]
            }
        ]
    },
    "display": {
        "bar": {}
    }
}