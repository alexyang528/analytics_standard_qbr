{
    "$id": "qbr_Page_Views_YoY",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Page Views YoY",
    "description": "Page Views by Month, Compared to the Previous Year",
    "query": {
        "metrics": [
            "yext/PAGE_VIEWS"
        ],
        "dimensions": [
            "yext/MONTHS"
        ],
        "filters": [
            {
                "filter": "yext/date",
                "in": "MONTHS_12"
            }
        ],
        "comparison": "PREVIOUS_PERIOD"
    },
    "display": {
        "line": {}
    }
}