{
    "$id": "qbr_Reviews_Volume_YoY",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Reviews Volume YoY",
    "description": "Reviews by Month, Compared to Previous Year",
    "query": {
        "metrics": [
            "yext/NEW_REVIEWS"
        ],
        "dimensions": [
            "yext/MONTHS"
        ],
        "filters": [
            {
                "filter": "yext/date",
                "in": "MONTHS_12"
            }
        ],
        "comparison": "PREVIOUS_YEAR"
    },
    "display": {
        "bar": {}
    }
}