{
    "$id": "qbr_Answers_Searches_and_Clicks",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Answers Searches and Clicks",
    "description": "Total Answers Clicks and Answers Searches by Month",
    "query": {
        "metrics": [
            "yext/ANSWERS_CLICKS",
            "yext/ANSWERS_SEARCHES"
        ],
        "dimensions": [
            "yext/MONTHS"
        ],
        "filters": [
            {
                "filter": "yext/date",
                "in": "MONTHS_13"
            },
            {
                "filter": "yext/ANSWERS_TRAFFIC_TYPE",
                "in": [
                    "EXTERNAL"
                ]
            }
        ]
    },
    "display": {
        "line": {}
    }
}